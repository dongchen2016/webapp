<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.demo.persistence.LevelOneMapper">
    <select id="insert"  resultType="java.lang.Integer">
        INSERT INTO TB_THINGS(USERID,LEVEL,STARTTIME,ENDTIME,CONTENT)
        VALUES(#{userid},#{level},#{startTime},#{endTime},#{content})
    </select>
</mapper>